{
    "manifest_version": 2,
    "name": "YouTube Old Design",
    "description": "Returns old YouTube design",
    "version": "1.4.8.8",
    "icons": {"128": "icon128.png"},
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": [
                "https://*.youtube.com/*",
                "https://*.youtu.be/*"
            ],
            "js": ["script.js"],
            "run_at": "document_start"
        }
    ]    
}
